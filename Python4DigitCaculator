print("Select Operation, +, -, X, /")
Operation_To_Do=input()
if Operation_To_Do==("+"):
    print("Select first number to add:")
    firstnumbertoadd=input()
    print("Select Second Number to add:")
    secondnumbertoadd=input()
    sumof1and2=int(firstnumbertoadd) + int(secondnumbertoadd)
    print(sumof1and2)
if Operation_To_Do==("-"):
    print("Select first number to subtract:")
    firstnumbertosubtract=input()
    print("Select Second Number to subtract:")
    secondnumbertosubtract=input()
    differenceof1and2=int(firstnumbertosubtract) - int(secondnumbertosubtract)
    print(differenceof1and2)
if Operation_To_Do==("X"):
    print("Select first number to multiply:")
    firstnumbertomultiply=input()
    print("Select Second Number to multiply:")
    secondnumbertomultiply=input()
    productof1and2=int(firstnumbertomultiply) + int(secondnumbertomultiply)
    print(productof1and2)
if Operation_To_Do==("/"):
    print("Select first number to divide:")
    firstnumbertodivide=input()
    print("Select Second Number to divide:")
    secondnumbertodivide=input()
    quotientof1and2=int(firstnumbertodivide) / int(secondnumbertodivide)
    print(quotientof1and2)
